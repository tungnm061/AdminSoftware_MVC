@{
    ViewBag.Title = "Search Ranking Amazon";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="panel panel-default">
    <div class="panel-heading">@ViewBag.Title</div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-4">
                <input type="text" value="" class="form-control" placeholder="Nhập tìm kiếm" id="TextSearch" style="height: 25px;" />
            </div>

            <div class="col-md-1 text-right">
                <button class="btn btn-primary btn-sm" id="btnSearchDateLeft">
                    <i class="glyphicon glyphicon-chevron-left"></i>
                </button>
            </div>
            <div class="col-md-1">
                <input type="number" value="1" class="form-control" id="Page" style="height: 25px;" />
            </div>
            <div class="col-md-1">
                <button class="btn btn-primary btn-sm" id="btnSearchDateRight">
                    <i class="glyphicon glyphicon-chevron-right"></i>
                </button>
            </div>
            <div class="col-md-2">
                <button class="btn btn-primary btn-sm" id="btnSearchDate">
                    <i class="glyphicon glyphicon-search"></i>
                    Tìm kiếm
                </button>
            </div>

        </div>
        <div class="row">
            <div class="col-md-12">
                <div id="grdMain" event-dbclick="1"></div>
            </div>
        </div>
    </div>
</div>
@section scripts
{
    <script type="text/javascript">
        var employees = @Html.Raw(Json.Encode(ViewBag.Users));
    </script>
    <script src="~/Areas/Sale/JsAction/ToolAmzSearch/index.js"></script>
}