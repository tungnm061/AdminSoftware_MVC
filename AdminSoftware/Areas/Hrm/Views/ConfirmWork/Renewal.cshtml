@model Entity.Kpi.WorkDetail

<div class="panel panel-default">
    <div class="panel-body">

        <div class="row">
            <div class="col-md-12">
                <div class="row padding-bottom-0">
                    <div class="col-md-4">
                        <label>Gia hạn đến ngày(*)</label>
                    </div>
                    <div class="col-md-8">
                        <input type="text" value="@Model.ToDate.ToString("dd/MM/yyyy")" class="form-control" id="ToDate"/>
                        <input type="hidden" value="@Model.WorkDetailId" class="form-control" id="WorkDetailId" />
                        <input type="hidden" value="@Model.WorkType" class="form-control" id="WorkType" />

                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
<div class="panel panel-default window-footer">
    <div class="panel-body">
        <div class="row">
            <div class="col-md-12 ">
                <button class="btn btn-primary" id="btnSaveRenewal" style="float: right">
                    <i class="glyphicon glyphicon-floppy-saved"></i>
                    Lưu
                </button>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">

</script>
<script src="~/Areas/Hrm/JsAction/Kpi/ConfirmWork/renewal.js"></script>




