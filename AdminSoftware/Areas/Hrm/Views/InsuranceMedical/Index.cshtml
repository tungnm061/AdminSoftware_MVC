
@{
    ViewBag.Title = "Bảo Hiểm Y Tế";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var year = (int) ViewBag.Year;
}
<div class="panel panel-default">
    <div class="panel-heading">@ViewBag.Title</div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-6">
                <div class="box-action">
                    <button class="btn btn-primary btn-sm" id="btnCreate">
                        <i class="glyphicon glyphicon-plus"></i>
                        Thêm
                    </button>
                    <button class="btn btn-primary btn-sm" id="btnEdit">
                        <i class="glyphicon glyphicon-edit"></i>
                        Sửa
                    </button>
                    <button class="btn btn-primary btn-sm" id="btnDelete">
                        <i class="glyphicon glyphicon-trash"></i>
                        Xóa
                    </button>
                </div>
            </div>
            <div class="col-md-6">
                <div class="col-md-3">
                    <input type="text" value="@year" id="Year"/>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div id="grdMain" event-dbclick="1"></div>
            </div>
        </div>
    </div>
</div>
@section scripts
{
    <script type="text/javascript">
        var departments = @Html.Raw(Json.Encode(ViewBag.Departments));
        var cities = @Html.Raw(Json.Encode(ViewBag.Cities));
        var employees = @Html.Raw(Json.Encode(ViewBag.Employees));
        var users = @Html.Raw(Json.Encode(ViewBag.Users));
        var medicals = @Html.Raw(Json.Encode(ViewBag.Medicals));
        var years = @Html.Raw(Json.Encode(ViewBag.Years));
    </script>
    <script src="~/Areas/Hrm/JsAction/InsuranceMedical/index.js"></script>
}

